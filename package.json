{
  "name": "@andyrmitchell/supabase-workflow",
  "version": "0.1.7",
  "description": "Helpers for a Supabase project",
  "type": "module",
  "files": [
    "dist",
    "assets",
    "README.MD",
    "LICENSE"
  ],
  "bin": {
    "sbw": "dist/sbw_cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build_release": "npm run build_prepare && np",
    "build": "tsup",
    "pkglint": "./build/publint_pipeable.sh",
    "build_prepare": "npm run build && npm run pkglint",
    "prepublishOnly": "npm run build_prepare",
    "test": "jest",
    "test:watch": "jest --watch",
    "test_cli": "npx tsup && node ./dist/sbw_cli.js",
    "test_cli:verbose": "npx tsup && node ./dist/sbw_cli.js --verbose"
  },
  "author": "andymitchell",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@supabase/supabase-js": "^2.43.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@andyrmitchell/file-io": "^0.10.5",
    "@andyrmitchell/typed-config": "^0.1.0",
    "@andyrmitchell/utils": "^0.4.3",
    "inquirer": "^9.2.23",
    "json5": "^2.2.3",
    "lodash-es": "^4.17.21",
    "pkg-dir": "^8.0.0"
  },
  "keywords": [
    "supabase",
    "guide",
    "workflow",
    "deployment",
    "troubleshooting"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andymitchell/supabase-workflow.git"
  }
}
